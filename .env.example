# ========================================
# ATUNA FRONTEND ENVIRONMENT CONFIGURATION
# ========================================
# 
# TEMPLATE FILE - Copy this to .env and update with your actual values
# This file shows all available environment variables with example values
# ========================================

# ========================================
# API Configuration
# ========================================
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=10000
VITE_API_VERSION=v1

# ========================================
# Application Information
# ========================================
VITE_APP_NAME=Atuna Real Estate Platform
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Comprehensive real estate management platform

# ========================================
# Environment Configuration
# ========================================
VITE_NODE_ENV=development
VITE_DEBUG_MODE=true
VITE_ENABLE_LOGGING=true

# ========================================
# Authentication Configuration
# ========================================
VITE_TOKEN_STORAGE_KEY=access_token
VITE_SESSION_TIMEOUT=30
VITE_ENABLE_REMEMBER_ME=true

# ========================================
# UI/UX Configuration
# ========================================
VITE_DEFAULT_THEME=system
VITE_ENABLE_THEME_SWITCH=true
VITE_DEFAULT_LOCALE=en-US
VITE_ENABLE_ANIMATIONS=true

# ========================================
# Feature Flags
# ========================================
VITE_ENABLE_CHATBOT=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SITE_VIEWING=true
VITE_ENABLE_CRM=true
VITE_ENABLE_MAINTENANCE_TRACKER=true
VITE_ENABLE_PAYMENT_DASHBOARD=true

# ========================================
# File Upload Configuration
# ========================================
VITE_MAX_FILE_SIZE=10
VITE_ALLOWED_FILE_TYPES=csv,xlsx,xls,jpg,jpeg,png,webp

# ========================================
# Map & Location Services
# ========================================
# VITE_MAP_API_KEY=your_map_api_key_here
VITE_DEFAULT_MAP_LAT=14.5995
VITE_DEFAULT_MAP_LNG=120.9842
VITE_DEFAULT_MAP_ZOOM=10

# ========================================
# Analytics & Tracking
# ========================================
# VITE_GA_TRACKING_ID=GA-XXXXX-X
VITE_ENABLE_ERROR_TRACKING=true

# ========================================
# Performance Configuration
# ========================================
VITE_ENABLE_SERVICE_WORKER=false
VITE_ENABLE_LAZY_LOADING=true
VITE_IMAGE_QUALITY=80

# ========================================
# Security Configuration
# ========================================
VITE_CORS_ORIGINS=http://localhost:3000,http://localhost:5173
VITE_ENABLE_CSRF_PROTECTION=true
VITE_CSP_LEVEL=moderate

# ========================================
# Development Tools
# ========================================
VITE_ENABLE_REACT_DEVTOOLS=true
VITE_ENABLE_REDUX_DEVTOOLS=true
VITE_ENABLE_HMR=true
